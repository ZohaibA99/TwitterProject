{
    "version" : 2,
    "builds": [
        {
            "src": "app/index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        },
        {
            "src": "tweets",
            "dest": "app/index.js"
        }
    ]
  }